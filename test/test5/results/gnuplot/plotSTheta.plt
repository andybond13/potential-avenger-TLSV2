datafile = './datFiles/stresstheta.dat'
stats datafile;
set title "Cohesive Link Stress vs. Theta Graph"
set xlabel "Theta (degrees)"
set ylabel "Stress (MPa)"
set xrange [0:360]
set terminal svg size 1200, 800 

set output './pngFiles/stressThetaCL.svg'
plot for[i=1:1:STATS_blocks] './datFiles/stresstheta.dat' index i-1 usi 4:5 ti 'Link Stress' w l;

